  ******************************************************************************
*/


#include "stm32f4xx.h"
			

int main(void)
{
	// 1. Reset the RCC clock config to the default reset state.
	// HSI On, PLL Off, system clock = cpu clock = 16MHz
	RCC_DeInit();

	// 2. update the SystemCoreClock variable
	SystemCoreClockUpdate();

	// 3. set up peripherals
	prvSetupHardware();

	for(;;);
}
